<%= form_with(model: LeasingAgent, local: true) do |form| %>
<h2>Sign Up for Leasing Agent</h2>

  <div class="field">
    <%= form.label 'Name' %>
    <%= form.text_field :name_field %>
  </div>
  <div class="field">
    <%= form.label 'Phone number' %>
    <%= form.text_field :phone_field %>
  </div>
  <div class="field">
    <%= form.label 'Email ID' %>
    <%= form.text_field :email_field %>
  </div>

  <div class="field">
    <%= form.label 'Password' %>
    <%= form.text_field :password_field %>
  </div>


<% end %>
